    "@types/express": "^4.17.11",
    "cors": "^2.8.5",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.4"
  },
  "dependencies": {
    "class-transformer": "^0.4.0",
